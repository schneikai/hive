%ul.list-inline
  - if controller_name != 'sessions'
    %li= link_to t('hive.links.new_session'), new_session_path(resource_name)
  - if devise_mapping.registerable? && controller_name != 'registrations'
    %li= link_to t('hive.links.new_registration'), new_registration_path(resource_name)
  - if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
    %li= link_to t('hive.links.new_password'), new_password_path(resource_name)
  - if devise_mapping.confirmable? && controller_name != 'confirmations'
    %li= link_to t('hive.links.new_confirmation'), new_confirmation_path(resource_name)
  - if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
    %li= link_to t('hive.links.new_unlock'), new_unlock_path(resource_name)
  %li= link_to t('hive.links.back'), root_path, class: 'hive-back'

= render "hive/shared/omniauth_links"
